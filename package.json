{
  "name": "@discipl/core-baseconnector",
  "version": "0.2.2",
  "description": "Discipl Core connector base",
  "main": "dist/base-connector.js",
  "module": "src/base-connector.js",
  "repository": "https://github.com/discipl/discipl-core-baseconnector",
  "author": "Bas Kaptijn",
  "license": "GPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/src",
    "/dist"
  ],
  "scripts": {

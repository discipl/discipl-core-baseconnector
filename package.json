    "@babel/register": "7.4.4",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "codecov": "3.7.1",
    "mocha": "5.2.0",
    "nyc": "14.1.1",
    "sinon": "7.3.2",
    "standard": "12.0.1"
  }
